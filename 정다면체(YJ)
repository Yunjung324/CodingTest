n, m = map(int, input().split())
a = []
max = -999999
s = set()
d = []
for i in range(1, n+1):
    for j in range(1, m+1):
        b = i + j
        a.append(b)
for i in range(len(a)):
    if max < a.count(a[i]):
        max = a.count(a[i])
    elif max == a.count(a[i]):
        s.add(a[i])
for i in s:
    print(i, end = ' ')
print()
